{
  "name": "q-r-mfed-sandbox",
  "packageManager": "yarn@4.2.1",
  "workspaces": [
    "packages/quasar-host",
    "packages/react-remote"
  ],
  "scripts": {
    "host:i": "cd packages/quasar-host && yarn",
    "remote:i": "cd packages/react-remote && yarn",
    "host:start": "yarn workspace quasar-host start",
    "remote:start": "yarn workspace react-remote start",
    "start": "yarn host:start && yarn remote:start",
    "host:build": "yarn workspace quasar-host build",
    "remote:build": "yarn workspace react-remote build",
    "build": "yarn host:build && yarn remote:build"
  }
}
